{
    "name": "arches/crontab",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arches\\Crontab\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "require": {
        "php": ">=5.6",
        "dragonmantank/cron-expression": "^3.3",
        "topthink/framework": "^5.0|^5.1|^6.0|^8.0",
        "ext-json": "*"
    },
    "config": {
        "allow-plugins": {
            "topthink/think-installer": true
        }
    },
    "extra": {
        "think": {
            "services": ["Arches\\Crontab\\Service\\Service"]
        }
    }
}
